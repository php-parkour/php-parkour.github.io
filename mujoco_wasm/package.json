  "dependencies": {
    "esbuild": "^0.25.12",
    "mujoco-js": "^0.0.7",
    "onnxruntime-web": "^1.21.1",
    "three": "^0.181.0"
  },
  "devDependencies": {
    "vite": "^6.2.2"
